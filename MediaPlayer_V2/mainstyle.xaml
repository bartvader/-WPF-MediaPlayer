						<GradientStop Color="#FFE1E1E1" Offset="0"/>
						<GradientStop Color="#FFEDEDED" Offset="0.2"/>
						<GradientStop Color="#FFEDEDED" Offset="0.8"/>
						<GradientStop Color="#FF9B4343" Offset="1"/>
					</LinearGradientBrush>
